    "@typescript-eslint/parser": "^4.7.0",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.1",
    "core-js": "^3.7.0",
