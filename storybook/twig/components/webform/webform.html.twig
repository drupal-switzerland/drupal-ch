<section {{ componentId ? 'id=' ~ (componentId | clean_class) : '' }} class="webform">
  <div class="inner">
    {% if title is not empty %}
      <h2 class="webform__title h2">{{ title }}</h2>
    {% endif %}
    <div class="webform__content">
      {{ drupal_entity('webform', webform.targetId) }}
    </div>
  </div>
</section>
