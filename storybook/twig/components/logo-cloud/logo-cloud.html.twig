<section {{ componentId ? 'id=' ~ (componentId | clean_class) : '' }} class="logo-cloud inner">
  {% if title %}
    <h2 class="logo-cloud__title h5">{{ title }}</h2>
  {% endif %}
  <div class="logo-cloud__logos">
    {% for logo in logos %}
      {% include '@twig/components/logo/logo.html.twig' with logo.entity only %}
    {% endfor %}
  </div>
</section>